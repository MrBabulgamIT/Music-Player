<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav"
    tools:ignore="UnusedNavigation"
    app:startDestination="@id/nav_home">


    <activity
        android:id="@+id/nav_home"
        android:name="com.example.com.music.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <activity
        android:id="@+id/nav_aboutus"
        android:name="com.example.com.music.AboutUs"
        android:label="activity_about_us"
        tools:layout="@layout/activity_about_us" />
    <activity
        android:id="@+id/nav_feedback"
        android:name="com.example.com.music.FeedbackActivity"
        android:label="activity_feedback"
        tools:layout="@layout/activity_feedback" />
